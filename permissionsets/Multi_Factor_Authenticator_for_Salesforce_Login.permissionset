<?xml version="1.0" encoding="UTF-8"?>
<PermissionSet xmlns="http://soap.sforce.com/2006/04/metadata">
    <description>Created the permission set as per DPS-18607 to force dual users to authenticate through Salesforce MFA during login.</description>
    <hasActivationRequired>false</hasActivationRequired>
    <label>Multi Factor Authenticator for Salesforce Login</label>
    <userPermissions>
        <enabled>true</enabled>
        <name>ForceTwoFactor</name>
    </userPermissions>
</PermissionSet>