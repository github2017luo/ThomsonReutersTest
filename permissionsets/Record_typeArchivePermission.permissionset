<PermissionSet xmlns="http://soap.sforce.com/2006/04/metadata">
    <description>Allow specific users with the “TR Business Administrator” profile to be able to change archived accounts (so the Legal data team can flip the archive record type to one of the un-archived ones)</description>
    <fieldPermissions>
        <editable>true</editable>
        <field>Account.Archive_Date__c</field>
        <readable>true</readable>
    </fieldPermissions>
    <fieldPermissions>
        <editable>false</editable>
        <field>Account.Change_Record_Type__c</field>
        <readable>true</readable>
    </fieldPermissions>
    <fieldPermissions>
        <editable>true</editable>
        <field>Account.Exclude_From_Archive__c</field>
        <readable>true</readable>
    </fieldPermissions>
    <fieldPermissions>
        <editable>true</editable>
        <field>Account.Exclude_from_Archive_Reason__c</field>
        <readable>true</readable>
    </fieldPermissions>
    <fieldPermissions>
        <editable>true</editable>
        <field>Account.Long_Term_Archive__c</field>
        <readable>true</readable>
    </fieldPermissions>
    <fieldPermissions>
        <editable>true</editable>
        <field>Account.ShortTerm_Archive_Remove_Date__c</field>
        <readable>true</readable>
    </fieldPermissions>
    <fieldPermissions>
        <editable>true</editable>
        <field>Account.Temp_Archive_Group__c</field>
        <readable>true</readable>
    </fieldPermissions>
    <hasActivationRequired>false</hasActivationRequired>
    <label>Record typeArchivePermission</label>
    <license>Salesforce</license>
    <recordTypeVisibilities>
        <recordType>Account.Archive</recordType>
        <visible>true</visible>
    </recordTypeVisibilities>
