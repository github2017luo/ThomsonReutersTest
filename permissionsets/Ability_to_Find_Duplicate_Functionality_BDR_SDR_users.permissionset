<?xml version="1.0" encoding="UTF-8"?>
<PermissionSet xmlns="http://soap.sforce.com/2006/04/metadata">
    <description>Permission set to allow BDR/SDR users to have the &quot;Find Duplicate&quot; button on the page layout to Merge leads which is accomplished by providing the &quot;Delete&quot; access in the Lead object</description>
    <hasActivationRequired>false</hasActivationRequired>
    <label>Ability to Find Duplicate Functionality - BDR/SDR users</label>
    <objectPermissions>
        <allowCreate>false</allowCreate>
        <allowDelete>true</allowDelete>
        <allowEdit>true</allowEdit>
        <allowRead>true</allowRead>
        <modifyAllRecords>false</modifyAllRecords>
        <object>Lead</object>
        <viewAllRecords>false</viewAllRecords>
    </objectPermissions>
</PermissionSet>