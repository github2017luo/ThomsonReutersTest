    <description>This permission will allow the transfer of record owner. Assign to ANZ Operations ONLY as workaround to Merge Accounts in S2E.</description>
    <hasActivationRequired>false</hasActivationRequired>
    <label>ANZ ONLY Transfer of Owner</label>
    <license>Salesforce</license>
    <userPermissions>
        <enabled>true</enabled>
        <name>TransferAnyEntity</name>
    </userPermissions>
    <userPermissions>
        <enabled>true</enabled>
        <name>UpdateWithInactiveOwner</name>
    </userPermissions>
</PermissionSet>