        function openARForm(formid,formname,caseid){
            var isnotfederal = JSON.parse('{!isnotfederal}');
            if (sforce.console.isInConsole()){             
             var PrimaryTabId;
             
             if(formname == 'IFF Adjustment' && isnotfederal == 'true'){
