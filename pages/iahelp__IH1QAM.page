                                            <div class="IH1ListItemFirstLine"><span class="IH1ListItemLabel">{!GlobalLocalisations['AdviceLabelListingSummary']} </span>{!Elems.Result.Summary}</div>
                                        </apex:commandLink>

                                        <apex:commandLink rendered="{!IF(Elems.Result.Name != '' && Elems.Result.Link != '' && isSF1 == true, true, false)}"
                                            action="{!getHelpTopic}"
                                            title="{!DialogueLocalisations['TipButtonViewTopic']}">
                                            
                                            <apex:param name="SearchResultId" assignTo="{!SearchResultId}" value="{!Elems.Element.HelpTopic__r.Id + '&IHContext=' + RecordId}"></apex:param>                   
                                            
                                            <div class="IH1ListItemHeader"><span class="IH1ListItemLabel">{!GlobalLocalisations['AdviceLabelListingName']} </span>{!IF(Elems.Element.iahelp__ElementType__c == 'Field' || Elems.Element.iahelp__ElementType__c == 'Custom Link' || Elems.Element.iahelp__ElementType__c == 'Related List Help Link', Elems.Element.iahelp__Identifier__c, Elems.Element.Name)}</div>
