                <div class="IH1Header{!IF(CalloutMode == true, ' IHHidden', '')}">
                    <div class="IH1HeaderIcon IHIcon30 IHIconHelpTopicIH1"></div>
                                        
                    <div class="IH1HeaderText">                    
                        <div class="IH1HeaderSuperscript">{!cH.TBrandTitle}</div>

                        <div class="IH1HeaderTitle">
                            <div class="IH1QAMTitleText IH1HeaderTitleText" id="PanelHelpListTitle">
                                {!HPLName}                              
                            </div>
                            <div class="IH1QAMTitleText IH1HeaderTitleText" id="PanelCalloutListTitle">
                                {!HPLName}                              
                            </div>
                            <div class="IH1QAMTitleText IH1HeaderTitleText" id="PanelHelpBookmarksTitle">
                                {!DialogueLocalisations['TitleSubtitleTabBookmarks']}
                            </div>
                            <div class="IH1QAMTitleText IH1HeaderTitleText" id="PanelHelpQuickLinksTitle">
                                {!DialogueLocalisations['TitleSubtitleTabQuickLinks']}
                            </div>
                            <div class="IH1QAMTitleText IH1HeaderTitleText" id="PanelHelpSearchTitle">
                                {!GlobalLocalisations['ButtonSearch']}
                            </div>
                            <div class="IH1QAMTitleText IH1HeaderTitleText" id="PanelIH1HelpToolsTitle">
                                {!GlobalLocalisations['TitleSubtitleTools']}
                            </div>
                        </div>
                    </div>

                </div>
<!--   
===============================================
NAV BAR
===============================================
 -->            
                <div id="IH1NavBar">                    
                    <span style="display: {!IF(Diags != '' && Diags != null, 'block', 'none')}; font-size: 1px; color: red;">{!Diags}</span>                    

                    <div id="HelpList"  title="{!DialogueLocalisations['TipTabHelpForCurrentPage']}" class="{!IF(CalloutMode == true, 'IHHidden ', '')}IH1NavBarItem{!IF(SelectedQAMTab=='PanelHelpList', ' IHActive', '')}">
                        <a class="IH1NavButton" href="javascript:void(0);">
                            <div class="IH1NavButtonIcon IHIcon30 IHIconHelpListIH1" ></div>
                        </a>                
                    </div>

                    <div id="CalloutList"  title="{!DialogueLocalisations['TipTabHelpForCurrentPage']}" class="{!IF(CalloutMode == true, '', 'IHHidden ')}IH1NavBarItem{!IF(SelectedQAMTab=='PanelCalloutList', ' IHActive', '')}">
                        <a class="IH1NavButton" href="javascript:void(0);">
                            <div class="IH1NavButtonIcon IHIcon30 IHIconHelpListIH1" ></div>
                        </a>                
                    </div>

                    <div id="HelpBookmarks" title="{!GlobalLocalisations['TipButtonOpenBookmarks']}" class="IH1NavBarItem{!IF(SelectedQAMTab=='PanelHelpBookmarks', ' IHActive', '')}">
                        <a class="IH1NavButton" href="javascript:void(0);">
                            <div class="IH1NavButtonIcon IHIcon30 IHIconBookmarksIH1" ></div>
                        </a>                
                    </div>
    
                    <div id="HelpQuickLinks" title="{!DialogueLocalisations['TipTabQuickLinks']}" class="IH1NavBarItem IHHidden">
                        <a class="IH1NavButton" href="javascript:void(0);">
                            <div class="IH1NavButtonIcon IHIcon30 IHIconQuickLinkIH1" ></div>
                        </a>
                    </div>                  

                    <div id="HelpSearch" title="{!DialogueLocalisations['TipTabSearch']}" class="IH1NavBarItem{!IF(SelectedQAMTab=='PanelHelpSearch', ' IHActive', '')}">
                        <a class="IH1NavButton" href="javascript:void(0);">
                            <div class="IH1NavButtonIcon IHIcon30 IHIconSearchIH1" ></div>
                        </a>                
                    </div>

                    <div id="IH1HelpHome" title="{!GlobalLocalisations['TipButtonHelpHome']}" class="IH1NavBarItem{!IF(DefaultHTSet == true, '', ' IHHidden')}">

                        <!--
                        NOTE SF1 / NON-SF1 / CALLOUT MODE LINK TARGET DIFFERENTIATION 
                        ------------------------------------------------------------- 
                         -->
                        <apex:commandLink rendered="{!isSF1 && !CalloutMode}" action="{!getHelpTopic}" >                            
                            <apex:param name="SearchResultId" assignTo="{!SearchResultId}" value="&IHContext={!RecordId}"></apex:param>                                               
                            <div class="IH1NavButtonIcon IHIcon30 IHIconHelpHomeIH1" ></div>
                        </apex:commandLink>

                        <apex:commandLink rendered="{!!isSF1}" action="{!getHelpTopic}" target="_Blank">                            
                            <apex:param name="SearchResultId" assignTo="{!SearchResultId}" value="&IHContext={!RecordId}"></apex:param>                                               
                            <div class="IH1NavButtonIcon IHIcon30 IHIconHelpHomeIH1" ></div>
                        </apex:commandLink>

                        <apex:commandLink rendered="{!CalloutMode}" action="{!getHelpTopic}" target="_Blank">                            
                            <apex:param name="SearchResultId" assignTo="{!SearchResultId}" value="&IHContext={!RecordId}"></apex:param>                                               
                            <div class="IH1NavButtonIcon IHIcon30 IHIconHelpHomeIH1" ></div>
                        </apex:commandLink>
                    </div>                                              

                    <div id="IH1HelpTools" title="{!GlobalLocalisations['TipTabTools']}" class="IH1NavBarItem{!IF(SelectedQAMTab=='PanelIH1HelpTools', ' IHActive', '')}">
                        <a class="IH1NavButton" href="javascript:void(0);">
                            <div class="IH1NavButtonIcon IHIcon30 IHIconHelpMenuIH1" ></div>
                        </a>                
                    </div>

                </div>
        

<!--   
===============================================
HELP FOR THIS PAGE LIST
===============================================
 -->            
                <div id="PanelHelpList" class="IH1Panel" style="display: none;">
                    <div class="IH1PanelBody">

                        <div class="IH1ListView">
                            <div id="ViewSelect">
                        
                                <div class="IH1ListViewLabel">
                                    {!DialogueLocalisations['AdviceLabelShowingHelpFor']}
                                </div>

                                <div id="IH1HelpFilterSelectors" style="float:left;">                               
                                    <div onclick="javascript:filterToElement(this);" id="selectorField" class="IH1Input IH1Button IH1Filter">
                                        {!DialogueLocalisations['ButtonFilterToFields']}
                                    </div>
                                    <div onclick="javascript:filterToElement(this);" id="selectorPageHelpLink" class="IH1Input IH1Button IH1Filter">
                                        {!SFObjPluralName}
                                    </div>
                                    <div onclick="javascript:filterToElement(this);" id="selectorButton" class="IH1Input IH1Button IH1Filter">
                                        {!DialogueLocalisations['ButtonFilterToButtons']}
                                    </div>
                                    <div onclick="javascript:filterToElement(this);" id="selectorCustomLink" class="IH1Input IH1Button IH1Filter">
                                        {!DialogueLocalisations['ButtonFilterToLinks']}
                                    </div>
                                    <div onclick="javascript:filterToElement(this);" id="selectorRelatedListHelpLink" class="IH1Input IH1Button IH1Filter">
                                        {!DialogueLocalisations['ButtonFilterToRelatedLists']}
                                    </div>
                                    <div onclick="javascript:filterToElement(this);" id="selectorAll" class="IH1Input IH1Button IH1Filter">
                                        {!DialogueLocalisations['ButtonFilterToEverything']}
                                    </div>
                                </div>                          

                            </div>
                        </div>

                        
                        <div class="IH1List" style="clear: both; overflow-y: auto; height: {!IF(isSF1 == true, 'auto', '355px')};">                              
                            <apex:Repeat value="{!HelpedElements}" var="Elems"> 
                                <div class="IH1ListItem IH1PageHelpItem {!SUBSTITUTE(Elems.Element.ElementType__c, ' ', '')}">
                                    
                                    <!--
                                    RESPOND TO "READ MORE" SETTING BY OFFERING 2x LISTINGS: ONE
                                    AS A LINK, ONE STYLED DIFFERENTLY AS JUST TEXT.
                                    ALSO, USE THE MOST "PLAIN ENGLISH" ELEMENT TEXT. IN SOME CASES
                                    THIS IS ELEMENT NAME, IN OTHERS IT'S IDENTIFIER... 
                                    
                                    ALSO, DIFFERENTIATE LINK TARGET BETWEEN SF1 (NO TARGET REQUIRED)
                                    AND PUBLISHER ACTION WHEN USED VIA "FEED" IN CLASSIC (TARGET A NEW BROWSER TAB SO TOPIC
                                    IS NOT CONSTRAINED TO THE MINIMAL AREA OF THE FEED)
                                     -->
                                     
                                    <div class="{!IF(Elems.Element.HelpTopic__r.ShowReadMoreLink__c == true, '', 'IHHidden')}">
                                        <apex:commandLink rendered="{!IF(Elems.Result.Name != '' && Elems.Result.Link != '' && isSF1 == false, true, false)}"
                                            action="{!getHelpTopic}"
                                            title="{!DialogueLocalisations['TipButtonViewTopic']}" 
                                            target="_Blank">
                                            
                                            <apex:param name="SearchResultId" assignTo="{!SearchResultId}" value="{!Elems.Element.HelpTopic__r.Id + '&IHContext=' + RecordId}"></apex:param>                   
                                            
