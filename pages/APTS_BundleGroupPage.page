                            values = item.Available_Values__c;
                        }
                    });

                    if(values){
                        return values.split(";");    
                    }

                    if(key == 'Contract Term'){
                        return $scope.AdjustmentValu;    
                    }

                    if(key == 'YOY +/-'){
                        return $scope.yoyPickVal;
                    }

                    if(key == 'Bundle Primary Material'){
                        return $scope.optionList;
