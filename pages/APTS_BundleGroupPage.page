                    });

                    return allStatusesAreNew && allow;
                };

                $scope.addLocation = function(item){
                    j$("[name=accountLocation]:checked").removeAttr("checked");

                    $scope.selectedLineItem = item;
                    $scope.selectedGroupForLocations = null;

                    j$('#myModal').modal();
