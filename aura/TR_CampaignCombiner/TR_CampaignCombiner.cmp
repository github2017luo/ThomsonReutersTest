    <aura:attribute name="isCombined" type="Boolean" default="false"/>
    <aura:attribute name="isCampaignCreated" type="Boolean" default="false"/>
    <aura:attribute name="newCreatedCampaignId" type="Id" />
    <aura:attribute name="combineresponsestate" type="List" />
    <aura:attribute name="selectedtableCampaign" type="String" />
    <aura:attribute name="CombineCurrentCampaign" type="Boolean" default="false"/>
    <aura:attribute name="CombineSelectedCampaign" type="Boolean" default="false"/>
    <aura:attribute name="currentcmpcheckbox" type="Boolean" default="false"/>
    <aura:attribute name="existingcmpcheckbox" type="Boolean" default="false"/>
