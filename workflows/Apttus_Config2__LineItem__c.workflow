            <type>FieldUpdate</type>
        </actions>
        <active>false</active>
        <description>This Workflow is required to allow user to change Adjustment amount once it is populated when record is created.</description>
        <formula>APTS_Proposal_Business_Unit__c = &apos;FindLaw&apos; &amp;&amp; ISPICKVAL(Apttus_Config2__LineStatus__c, &apos;Renewed&apos;) &amp;&amp; APTS_Product_Code__c = &apos;40656701&apos; &amp;&amp; ISNULL( Apttus_Config2__AdjustmentAmount__c ) &amp;&amp; (!$Setup.GLI_OC_Bulk_Load__c.APTS_Disable_Workflow_Rule__c)</formula>
        <triggerType>onAllChanges</triggerType>
