    <fields>
        <fullName>Monthly_Charge_Calc__c</fullName>
        <externalId>false</externalId>
        <formula>IF(ISPICKVAL(Product2.Subscribable_Indicator__c,&quot;Subscribable&quot;) &amp;&amp; (ISPICKVAL(  Quote.Opportunity.Sales_Org__c  ,&quot;LLEY&quot;) || ISPICKVAL( Quote.Opportunity.Sales_Org__c ,&quot;ABEP&quot;) || ISPICKVAL( Quote.Opportunity.Sales_Org__c ,&quot;BEJE&quot;) || ISPICKVAL( Quote.Opportunity.Sales_Org__c ,&quot;LLPA&quot;) || ISPICKVAL( Quote.Opportunity.Sales_Org__c ,&quot;LLUR&quot;))
, Floor((Total_Price_Calculation__c / Quote.Subscription_Term__c)* (10 ^ Decimal_Places_for_Currency__c))/ (10 ^ Decimal_Places_for_Currency__c) ,0)</formula>
        <label>Monthly Charge Calc</label>
        <precision>18</precision>
        <required>false</required>
        <scale>3</scale>
        <type>Currency</type>
    </fields>
    <fields>
        <fullName>Monthly_Charge_Suggested_Price__c</fullName>
        <description>The Suggested Total Price be displayed in a popup to the user if there is a Monthly Charge Variance.</description>
        <externalId>false</externalId>
        <formula>Floor( 
( Total_Price_Calculation__c / Quote.Subscription_Term__c)* (10^  Decimal_Places_for_Currency__c  ) 
)/(10^Decimal_Places_for_Currency__c) * Quote.Subscription_Term__c</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>MC Suggested Price</label>
        <precision>18</precision>
        <required>false</required>
        <scale>2</scale>
        <type>Currency</type>
    </fields>
    <fields>
        <fullName>Monthly_Charge_Suggested_Total_Price_Msg__c</fullName>
        <externalId>false</externalId>
        <formula>IF( 
AND( 
ISPICKVAL(PricebookEntry.Product2.Subscribable_Indicator__c,&quot;Subscribable&quot;), 
(OR(ISPICKVAL( $User.Sales_Org__c ,&quot;LLEY&quot;),
ISPICKVAL( $User.Sales_Org__c ,&quot;LLPA&quot;),
ISPICKVAL( $User.Sales_Org__c ,&quot;LLUR&quot;),
ISPICKVAL( $User.Sales_Org__c ,&quot;ABEP&quot;),
ISPICKVAL( $User.Sales_Org__c ,&quot;BEJE&quot;))), 
Monthly_Charge_Variance_Amount__c &lt;&gt; 0), 
TEXT(Monthly_Charge_Suggested_Price__c),&quot;&quot;)</formula>
        <label>Suggested Total Price</label>
        <required>false</required>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Monthly_Charge_Variance_Amount__c</fullName>
        <description>Value used to determine if a different suggested price is required to be given to the user to accommodate Monthly Charge rounding.</description>
        <externalId>false</externalId>
        <formula>IF(
ISPICKVAL(PricebookEntry.Product2.Subscribable_Indicator__c,&quot;Subscribable&quot;)
,(Floor(Total_Price_Calculation__c* (10^ Decimal_Places_for_Currency__c ))/(10^Decimal_Places_for_Currency__c)) -(Monthly_Charge_Calc__c * Quote.Subscription_Term__c)
,0)</formula>
        <label>MC Variance Amount</label>
        <precision>18</precision>
        <required>false</required>
        <scale>2</scale>
        <type>Currency</type>
    </fields>
