    <validationRules>
        <fullName>Annual_Price_Non_Editable</fullName>
        <active>true</active>
        <description>Created as part of SFD-415</description>
        <errorConditionFormula>AND( 
OR($Profile.Name = &quot;LatAm User&quot;,$Profile.Name = &quot;Partner Portal User&quot;), 
NOT(ISBLANK(Annual_Price__c ) ), 
OR( 
Quote.Renewal_Flag_Formula__c = true, 
AND( 
Quote.Short_Term_Sub__c &lt;&gt; true, 
AND( 
Quote.Sales_Org__c &lt;&gt; &quot;RV01&quot;, 
Quote.Sales_Org__c &lt;&gt; &quot;TSL&quot;, 
Quote.Sales_Org__c &lt;&gt; &quot;LPTC&quot;, 
Quote.Sales_Org__c &lt;&gt; &quot;LPCL&quot; 
) 
) 
) 
)</errorConditionFormula>
        <errorMessage>The Subscription is not marked as Non Annual. Please remove the value from the Annual Price field.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Annual_Price_Non_Editable_W_O_Short_Term</fullName>
        <active>true</active>
        <description>Created as part of SFD-415</description>
        <errorConditionFormula>AND( 
OR($Profile.Name = &quot;LatAm User&quot;,$Profile.Name = &quot;Partner Portal User&quot;), 
NOT(ISBLANK(Annual_Price__c )), 
Quote.Short_Term_Sub__c &lt;&gt; true, 
OR(	Quote.Sales_Org__c = &quot;RV01&quot;, 
Quote.Sales_Org__c = &quot;TSL&quot;, 
Quote.Sales_Org__c = &quot;LPTC&quot;, 
Quote.Sales_Org__c = &quot;LPCL&quot; 
) 
)</errorConditionFormula>
        <errorMessage>The Annual Price field should not be populated for Full Term Subscriptions. If this is a Non Annual Subscription please check the “Non Annual Sub” field on the header before populating Annual Price.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Annual_Price_Req_for_Short_Term_Sub</fullName>
        <active>true</active>
        <description>Created as part of SFD-415</description>
        <errorConditionFormula>AND( 
OR($Profile.Name = &quot;LatAm User&quot;,$Profile.Name = &quot;Partner Portal User&quot;), 
OR(Quote.Sales_Org__c = &quot;RV01&quot;, 
Quote.Sales_Org__c = &quot;TSL&quot;, 
Quote.Sales_Org__c = &quot;LPTC&quot;, 
Quote.Sales_Org__c = &quot;LPCL&quot;), 
ISBLANK( Annual_Price__c ),Quote.Short_Term_Sub__c = true, Quote.Renewal_Flag_Formula__c &lt;&gt; true 
)</errorConditionFormula>
        <errorMessage>The Annual Price field is required for Non Annual Subscriptions.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>LATAM_QLI_LOCK_AT_0</fullName>
        <active>true</active>
        <description>Lock QLI at 0%</description>
        <errorConditionFormula>AND(
$User.Username &lt;&gt;  $Label.InformaticaUserName ,
	NOT(ISPICKVAL( Quote.Opportunity.Sales_Org__c ,&quot;SFWS&quot;)), 
	NOT(ISPICKVAL( Quote.Opportunity.Sales_Org__c ,&quot;SCSI&quot;)), 
	NOT(ISPICKVAL( Quote.Opportunity.Sales_Org__c ,&quot;TSCE&quot;)), 
	NOT(ISPICKVAL( Quote.Opportunity.Sales_Org__c ,&quot;EDIC&quot;)), 
	ISPICKVAL(Quote.Opportunity.StageName,&quot;0%&quot;), 
	OR( $Profile.Name = &quot;LatAm User&quot;, $Profile.Name = &quot;Partner Portal User&quot;), 
	$UserRole.Name &lt;&gt; &quot;LatAm Order Fulfillment&quot;, 
	Quote.RecordType.Name = &apos;LATAM Renewal Quote&apos;,
	Quote.Opportunity.Renewal_Flag__c = true
)</errorConditionFormula>
        <errorMessage>If the opportunity is at a stage of 0%, Quote Line Item cannot be edited</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>LATAM_QLI_Lock_to_Sales_at_Opty_95_100</fullName>
        <active>true</active>
        <description>Quote Line Item should be fully locked if the Opportunity is 95% and 100%.</description>
        <errorConditionFormula>AND(
        $User.Username &lt;&gt;  $Label.InformaticaUserName ,
	OR(ISPICKVAL(Quote.Opportunity.StageName,&quot;100%&quot;),ISPICKVAL(Quote.Opportunity.StageName,&quot;95%&quot;)) ,
	OR($Profile.Name = &quot;LatAm User&quot;,$Profile.Name = &quot;Partner Portal User&quot;),
	$UserRole.DeveloperName &lt;&gt; &quot;LatAm_Order_Fulfillment&quot;, 
	NOT(ISPICKVAL(Quote.Opportunity.Oppty_Mgmt_Type__c, &quot;Transaction Summary&quot;))
)</errorConditionFormula>
        <errorMessage>If the opportunity is at a stage of 95% or 100%, Line Item cannot be edited by sales; only by the Order processing team.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>LATAM_Renewals_QLI_Lock_at_Quote_Remove</fullName>
        <active>true</active>
        <description>Locking Quote Line Item when the Quote is Removed.</description>
        <errorConditionFormula>AND(
	$User.Username &lt;&gt; $Label.InformaticaUserName , 
	OR( $Profile.Name = &quot;LatAm User&quot;, $Profile.Name = &quot;Partner Portal User&quot;),	
	Quote.Renewal_Flag_Formula__c = true,	
	OR(			
		AND( 
			Quote.RecordType.DeveloperName = &apos;LATAM_Renewal_Quote&apos;,
			ISPICKVAL(Quote.Approval_Status__c, &quot;Pending Cancellation Approval&quot;),			
			Quote.IsSkipQtyLockingValidation__c = false	
		), 
		AND( 
			Quote.RecordType.DeveloperName = &apos;LATAM_Renewal_Quote&apos;,
			ISPICKVAL(Quote.Approval_Status__c, &quot;Cancellation Approved&quot;),			
			Quote.IsSkipQtyLockingValidation__c = false, 
			$UserRole.DeveloperName &lt;&gt; &quot;LatAm_Order_Fulfillment&quot; 
		), 
		AND( 
			Quote.RecordType.DeveloperName = &apos;LATAM_Renewal_Quote&apos;,
			ISPICKVAL(Quote.Status, &quot;Order Cancelled&quot;),			
			Quote.IsSkipQtyLockingValidation__c = false 			
		),
		AND( 
			Quote.RecordType.DeveloperName = &apos;LATAM_Remove_Renewal_Quote&apos;, 
			ISPICKVAL(Quote.Status, &quot;Remove&quot;), 			
			Quote.IsSkipQtyLockingValidation__c = false 
		) 
	)
)</errorConditionFormula>
        <errorMessage>If the Quote is Cancelled or Removed, Line Item cannot be edited.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>LatAm_Quote_LI_Subscription_Date_Sequenc</fullName>
        <active>true</active>
        <description>For application LatAm Profiles  only this requires that the Expected Revenue Date be Prior to the Expiration Date if Option 1:  Product has Subscribable Indicator value of “Subscribable” or “Both”
Option 2:  The Opportunity Category is SUSCRIPCION</description>
        <errorConditionFormula>AND( 
$User.Username &lt;&gt; $Label.InformaticaUserName ,
OR( 
$Profile.Name = &quot;LatAm User&quot;, 
$Profile.Name = &quot;Partner Portal User&quot;),
OR(Quote.RecordType.Name = &apos;LATAM New Quote&apos;,Quote.RecordType.Name = &apos;LATAM Renewal Quote&apos;),  
OR(Quote.Opportunity.Has_LatAm_Stabilization_Primary__c &gt; 0, Quote.Opportunity.All_Quote_Count__c = 0,Quote.RecordType.Name = &apos;LATAM Renewal Quote&apos;), 
Quote.Opportunity.Expected_Revenue_Date__c &gt;  Quote.Opportunity.Expiration_Date__c ,
ISPICKVAL(Product2.Subscribable_Indicator__c, &quot;Subscribable&quot;)
)</errorConditionFormula>
        <errorMessage>Expected Revenue Date cannot be more than or equal to the Expiration Date. Please, review and correct the dates in those fields.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>LatAm_Quote_LI_Subscription_Dates_Req</fullName>
        <active>true</active>
        <description>For only LatAm Profiles (Record Type excludes in-flight quote) this requires that the Expected Revenue Date and Expiration Date be populated if Product has Subscribable Indicator value of “Subscribable” 
Option 2:  The Opportunity Category is SUSCRIPCION</description>
        <errorConditionFormula>AND( 
 $User.Username &lt;&gt;  $Label.InformaticaUserName ,
 OR (
 ISPICKVAL( Quote.Opportunity.Sales_Org__c ,&quot;LLEY&quot;), 
 ISPICKVAL( Quote.Opportunity.Sales_Org__c ,&quot;LLPA&quot;),
 ISPICKVAL( Quote.Opportunity.Sales_Org__c ,&quot;LLUR&quot;),
 ISPICKVAL( Quote.Opportunity.Sales_Org__c ,&quot;ABEP&quot;),
 ISPICKVAL( Quote.Opportunity.Sales_Org__c ,&quot;BEJE&quot;),
 ISPICKVAL( Quote.Opportunity.Sales_Org__c ,&quot;DFCL&quot;)
 ),
  
OR( 
$Profile.Name = &quot;LatAm User&quot;, 
$Profile.Name = &quot;Partner Portal User&quot;), 
OR(
Quote.Opportunity.Has_LatAm_Stabilization_Primary__c &gt; 0, Quote.Opportunity.All_Quote_Count__c = 0,Quote.RecordType.Name = &apos;LATAM Renewal Quote&apos;),
OR(Quote.RecordType.Name = &apos;LATAM New Quote&apos;,Quote.RecordType.Name = &apos;LATAM Renewal Quote&apos;),  
OR( 
ISNULL(Quote.Opportunity.Expected_Revenue_Date__c ), 
ISNULL( Quote.Opportunity.Expiration_Date__c )), 
ISPICKVAL(Product2.Subscribable_Indicator__c, &quot;Subscribable&quot;)
)</errorConditionFormula>
        <errorMessage>Expected Revenue Date and Expiration Date cannot be blank. Please, review and include a value for both fields.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Lock_Quote_Line_Item</fullName>
        <active>true</active>
        <description>Lock user from making changes once quote is approved</description>
        <errorConditionFormula>AND(
Quote.Pricing_Approval_Complete__c = True,
OR(
ISCHANGED(Discount),
ISCHANGED(UnitPrice),
ISCHANGED(Quantity),AND(ISCHANGED( Annual_Price__c ), Quote.Renewal_Flag_Formula__c &lt;&gt; true )),
Quote.Is_Child_Quote_MSaf__c &lt;&gt; true
)</errorConditionFormula>
        <errorMessage>You cannot modify Discount, Sales Price, Annual Price or Quantity once Quote pricing approval is completed. To proceed with modification, please click “Unlock line item” button.</errorMessage>
    </validationRules>
