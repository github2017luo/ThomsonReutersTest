<?xml version="1.0" encoding="UTF-8"?>
<CustomObject xmlns="http://soap.sforce.com/2006/04/metadata">
    <customSettingsType>List</customSettingsType>
    <description>Created for MCL-1576. This custom settings is utilized in CreateCustomer class to pick the countries where Informatica overwrites Billing State/Province field with the actual Region name instead of Region Code</description>
    <enableFeeds>false</enableFeeds>
    <fields>
        <fullName>Country__c</fullName>
        <description>Displays the country name for the corresponding Country code stored in the Name field</description>
        <externalId>false</externalId>
        <label>Country</label>
        <length>40</length>
        <required>true</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <label>Country BigRule Exception</label>
    <visibility>Public</visibility>
</CustomObject>
