    <validationRules>
        <fullName>Expiry_Date_Must_be_Greater_than_Today</fullName>
        <active>true</active>
        <description>For Account Alerts the Expiry Date cannot be less than creation date.</description>
        <errorConditionFormula>AND( 
$Profile.Name &lt;&gt; &apos;System Administrator&apos;, 
OR( 
AND(ISNEW(), Account_Alert_Expiry__c &lt; Today()), 
AND(NOT(ISNEW()), Account_Alert_Expiry__c &lt; DATEVALUE(CreatedDate)) 
) 
)</errorConditionFormula>
        <errorDisplayField>Account_Alert_Expiry__c</errorDisplayField>
        <errorMessage>Account Alert Expiry Date must be greater than the Alert creation date.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Expiry_Date_must_be_less_than_18_months</fullName>
        <active>true</active>
        <description>For Account Alerts with an Alert Category of ‘Sales’, ‘Customer Service’ or ‘Financial’, the Expiry Date cannot exceed 18 months from creation date.  Exception: Alerts created via integration.</description>
        <errorConditionFormula>AND( 
/*$Profile.Name &lt;&gt; &apos;System Administrator&apos;,*/ 
NOT(ISNULL(TEXT(Alert_Category__c))),
NOT(ISPICKVAL(Alert_Category__c, &quot;Governance&quot;)), 
NOT(ISBLANK(Account_Alert_Expiry__c)),
Account_Alert_Expiry__c &gt; Today() + 548,  
ISBLANK(Source_System__c) 
)</errorConditionFormula>
        <errorDisplayField>Account_Alert_Expiry__c</errorDisplayField>
        <errorMessage>Expiry Date cannot exceed 18 months from creation date.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Expiry_Date_must_be_populated</fullName>
        <active>true</active>
        <description>For Account Alerts with an Alert Category of ‘Sales’, ‘Customer Service’ or ‘Financial’, the Expiry Date cannot be blank. Exception: Alerts created via integration.</description>
        <errorConditionFormula>AND( 
/*$Profile.Name &lt;&gt; &apos;System Administrator&apos;,*/
NOT(ISNULL(TEXT(Alert_Category__c))), 
NOT(ISPICKVAL(Alert_Category__c, &quot;Governance&quot;)), 
ISBLANK(Account_Alert_Expiry__c),
ISBLANK( Source_System__c ) 
)</errorConditionFormula>
        <errorDisplayField>Account_Alert_Expiry__c</errorDisplayField>
        <errorMessage>Expiry Date cannot be blank.</errorMessage>
    </validationRules>
