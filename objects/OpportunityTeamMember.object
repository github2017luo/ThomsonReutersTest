    <validationRules>
        <fullName>LAtam_Restrict_User_Modifying_Opty_Team</fullName>
        <active>true</active>
        <description>DPS-18481 - Restrict Users from Modifying Opportunity Team when the opportunity is Stage = 100%, 95%, 0%</description>
        <errorConditionFormula>AND($Setup.LTS_Disable_Application_Automation__c.LTS_Disable_Validation_Rules__c =false,
NOT(Opportunity.Is_GLI__c), OR(ISPICKVAL(Opportunity.StageName,&apos;100%&apos;), ISPICKVAL(Opportunity.StageName,&apos;95%&apos;), ISPICKVAL(Opportunity.StageName,&apos;0%&apos;)),
OR($Profile.Name = &apos;LatAm User&apos;,$Profile.Name = &apos;Partner Portal User&apos;),
NOT($Permission.LatAm_Partner_Channel_Team ))</errorConditionFormula>
        <errorMessage>The Opportunity Team cannot be modified at Stage 95%, 100% or 0%</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>LatAm_PartnerRoleUpdateCheck</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
	$Setup.LTS_Disable_Application_Automation__c.LTS_Disable_Validation_Rules__c =false,
        NOT(Opportunity.Is_GLI__c),
        OR($Profile.Name == &apos;LatAm User&apos;,$Profile.Name==&apos;Partner Portal User&apos;),
	NOT( $Permission.LatAm_Partner_Channel_Team ), 
	ISCHANGED(Partner_Role__c),
	OR(
		AND(
			INCLUDES(Partner_Role__c,&apos;Partner Deal Hunter&apos;),
			NOT(INCLUDES(PRIORVALUE(Partner_Role__c),&apos;Partner Deal Hunter&apos;))
		),
		AND(
			NOT(INCLUDES(Partner_Role__c,&apos;Partner Deal Hunter&apos;)),
			INCLUDES(PRIORVALUE(Partner_Role__c),&apos;Partner Deal Hunter&apos;)
		)
	)
)</errorConditionFormula>
        <errorMessage>You can not change a Partner to Partner Deal Hunter.  Please reach out to Partner Management team.</errorMessage>
    </validationRules>
