        <fullName>checkbatchcount</fullName>
        <active>true</active>
        <description>It checks the number of records to be set for running in Digital Cohort execution Job</description>
        <errorConditionFormula>AND( 
NOT(ISBLANK( Batch_Size__c )), 
OR(
Batch_Size__c &gt; 5,
Batch_Size__c &lt; 1 
  )
)</errorConditionFormula>
