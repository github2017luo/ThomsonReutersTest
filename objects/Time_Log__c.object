    <fields>
        <fullName>DurationToNow__c</fullName>
        <description>Time value for start of record to Now()</description>
        <externalId>false</externalId>
        <formula>IF( 
     (Start_Time__c =  Stop_Time__c) , 
     Duration_Minutes__c,
     IF(
         Duration_Minutes__c &gt; 0, 
         Duration_Minutes__c,
           (FLOOR(Now() - Start_Time__c) *1440  +
            ROUND(MOD((Now() - Start_Time__c)*24,24),0) * 60  +
            ROUND(MOD((Now() - Start_Time__c)*1440,60),2)
            )
        )
   )</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>DurationToNow</label>
        <precision>18</precision>
        <required>false</required>
        <scale>2</scale>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Number</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Duration_Hours__c</fullName>
        <description>Duration of the Case in hours</description>
        <externalId>false</externalId>
        <formula>Duration_Minutes__c / 60</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <inlineHelpText>Duration of the Case in hours</inlineHelpText>
        <label>Duration Hours</label>
        <precision>18</precision>
        <required>false</required>
        <scale>1</scale>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Number</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Duration_Minutes__c</fullName>
        <externalId>false</externalId>
        <formula>FLOOR(Stop_Time__c - Start_Time__c) *1440
+
ROUND(MOD((Stop_Time__c - Start_Time__c)*24,24),0) * 60
+
ROUND(MOD((Stop_Time__c - Start_Time__c)*1440,60),2)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Duration Minutes</label>
        <precision>18</precision>
        <required>false</required>
        <scale>2</scale>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Number</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Duration__c</fullName>
        <externalId>false</externalId>
        <formula>IF(ISBLANK(Stop_Time__c),&apos;&apos; ,TEXT(
FLOOR(Stop_Time__c - Start_Time__c)
) &amp; &quot; Day(s) &quot; &amp;
TEXT(
FLOOR(MOD((Stop_Time__c - Start_Time__c)*24,24))
) &amp;&quot; Hour(s) &quot; &amp;
TEXT(
ROUND(MOD((Stop_Time__c - Start_Time__c)*1440,60),0)
) &amp;&quot; Minute(s) &quot; )</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Duration</label>
        <required>false</required>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
