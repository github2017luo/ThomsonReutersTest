    <validationRules>
        <fullName>Restrict_Change_of_Contract_Incremental</fullName>
        <active>true</active>
        <errorConditionFormula>AND($Profile.Name = &apos;ANZ Users&apos;, NOT(ISPICKVAL(Status,&quot;Draft&quot;)), 
ISCHANGED(ANZ_Contract_Incremental__c) )</errorConditionFormula>
        <errorMessage>Contract Incremental field can&apos;t be changed at this Status</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Restrict_Contract_Signed_Status_Update</fullName>
        <active>true</active>
        <description>Created as part of MCL-2190</description>
        <errorConditionFormula>AND($Setup.LTS_Disable_Application_Automation__c.LTS_Disable_Validation_Rules__c = false,$Profile.Name = &apos;ANZ Users&apos;, 
ISCHANGED(Status), 
ISPICKVAL(Status,&quot;Signed&quot;), 

OR(AND( 
ISBLANK( CustomerSignedDate), 
ISPICKVAL(PRIORVALUE(Status),&quot;Approved&quot;)), 
NOT(ISPICKVAL(PRIORVALUE(Status),&quot;Approved&quot;))) 

)</errorConditionFormula>
        <errorMessage>Contract Status Can&apos;t be changed to Signed</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Restrict_Contract_Status_Update</fullName>
        <active>true</active>
        <description>Created as part of MCL-2190</description>
        <errorConditionFormula>AND($Setup.LTS_Disable_Application_Automation__c.LTS_Disable_Validation_Rules__c = false,$Profile.Name = &apos;ANZ Users&apos;, 
ISCHANGED(Status), 
OR( ISPICKVAL(Status,&quot;In Approval Process&quot;), 
ISPICKVAL(Status,&quot;Approved&quot;), 
ISPICKVAL(Status,&quot;Rejected&quot;), 
ISPICKVAL(Status,&quot;Pending Approval&quot;) 
), 

OR( ISPICKVAL(PRIORVALUE(Status),&quot;Approved&quot;), 
ISPICKVAL(PRIORVALUE(Status),&quot;Rejected&quot;), 
ISPICKVAL(PRIORVALUE(Status),&quot;Signed&quot;) 
) 
)</errorConditionFormula>
        <errorMessage>Contract Status Can&apos;t be change at this stage</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Restricting_Change_of_Contract_Renewal</fullName>
        <active>true</active>
        <errorConditionFormula>AND($Profile.Name = &apos;ANZ Users&apos;, NOT(ISPICKVAL(Status,&quot;Draft&quot;)),
 ISCHANGED(ANZ_Contract_Renewal__c) )</errorConditionFormula>
        <errorMessage>Contract Renewal field can&apos;t be changed at this Status</errorMessage>
    </validationRules>
