    <fields>
        <fullName>LCRM_Total_Annualized_Price__c</fullName>
        <description>Total Annualized Price</description>
        <externalId>false</externalId>
        <formula>IF(ContractTerm = 0, 0, 
	IF(TEXT(LCRM_Transaction_Type__c) = &apos;Bridge&apos;, 
		IF((LCRM_Total_Period_Price_2__c + LCRM_Total_Period_Price_3__c + LCRM_Total_Period_Price_4__c + LCRM_Total_Period_Price_5__c) = 0, 0, ((LCRM_Total_Period_Price_2__c + LCRM_Total_Period_Price_3__c + LCRM_Total_Period_Price_4__c + LCRM_Total_Period_Price_5__c)/( FLOOR(ContractTerm/12) ))),
		IF(LCRM_Total_Price__c = 0, 0, ((LCRM_Total_Price__c/ContractTerm) * 12)))
)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <inlineHelpText>Total Annualized Price</inlineHelpText>
        <label>Total Annualized Price</label>
        <precision>18</precision>
        <required>false</required>
        <scale>2</scale>
        <trackHistory>false</trackHistory>
        <type>Number</type>
        <unique>false</unique>
    </fields>
