<?xml version="1.0" encoding="UTF-8"?>
<CustomObject xmlns="http://soap.sforce.com/2006/04/metadata">
    <customSettingsType>Hierarchy</customSettingsType>
    <description>It is used by formula on asset line item to retrieve unique number associated with assets.</description>
    <enableEnhancedLookup>false</enableEnhancedLookup>
    <enableFeeds>false</enableFeeds>
    <fields>
        <fullName>Auto_Format__c</fullName>
        <description>Format Text - to be used as prefix</description>
        <externalId>false</externalId>
        <label>Auto Format</label>
        <length>10</length>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Latam_Latest_Number__c</fullName>
        <description>Latest number will be used to create auto asset number</description>
        <externalId>false</externalId>
        <label>Latest Number</label>
        <precision>8</precision>
        <required>false</required>
        <scale>0</scale>
        <trackTrending>false</trackTrending>
        <type>Number</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Separator__c</fullName>
        <description>Use this separator to split the Numbers</description>
        <externalId>false</externalId>
        <label>Separator</label>
        <length>4</length>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Starting_Number__c</fullName>
        <externalId>false</externalId>
        <label>Starting Number</label>
        <precision>8</precision>
        <required>false</required>
        <scale>0</scale>
        <trackTrending>false</trackTrending>
        <type>Number</type>
        <unique>false</unique>
    </fields>
    <label>Asset Line Item Auto Number</label>
    <visibility>Public</visibility>
</CustomObject>
