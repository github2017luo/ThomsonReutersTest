<?xml version="1.0" encoding="UTF-8"?>
<CustomObject xmlns="http://soap.sforce.com/2006/04/metadata">
    <customSettingsType>List</customSettingsType>
    <description>Stores the Product Legal language settings for Findlaw</description>
    <enableFeeds>false</enableFeeds>
    <fields>
        <fullName>APTS_Product_Codes__c</fullName>
        <description>Holds the comma separated list of product codes</description>
        <externalId>false</externalId>
        <label>Product Codes</label>
        <length>255</length>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>APTS_Product_Names__c</fullName>
        <description>Holds the comma separated list of product names</description>
        <externalId>false</externalId>
        <label>Product Names</label>
        <length>255</length>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>APTS_Short_Term_Applicable__c</fullName>
        <defaultValue>false</defaultValue>
        <description>Tells whether the name is applicable for Short term or long term</description>
        <externalId>false</externalId>
        <label>Short Term Applicable</label>
        <trackTrending>false</trackTrending>
        <type>Checkbox</type>
    </fields>
    <fields>
        <fullName>APTS_Short_Term_Not_Applicable__c</fullName>
        <defaultValue>false</defaultValue>
        <description>Holds true when developername for the record is not applicable for Short term variants and does not have a short term variant name</description>
        <externalId>false</externalId>
        <label>Short Term Not Applicable</label>
        <trackTrending>false</trackTrending>
        <type>Checkbox</type>
    </fields>
    <label>Product Legal Language FL Settings</label>
    <visibility>Public</visibility>
</CustomObject>
