        <unique>false</unique>
    </fields>
    <fields>
        <fullName>MaritzCX__Create_New_Contact_On_Survey_Response__c</fullName>
        <defaultValue>false</defaultValue>
        <deprecated>false</deprecated>
        <description>When enabled a new Contact/PersonAccount will be created for a Response for which an associated Contact/PersonAccount is not found. This setting will work only if &apos;Insert Unmapped Responses&apos; setting is enabled.</description>
        <externalId>false</externalId>
