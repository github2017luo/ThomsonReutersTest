<?xml version="1.0" encoding="UTF-8"?>
<CustomObject xmlns="http://soap.sforce.com/2006/04/metadata">
    <customSettingsType>List</customSettingsType>
    <description>A list of regions grouped by country that is presented to the user when editing an address</description>
    <enableEnhancedLookup>false</enableEnhancedLookup>
    <enableFeeds>false</enableFeeds>
    <fields>
        <fullName>Country_Code__c</fullName>
        <description>The ISO country code or abbreviation for the country for the given region</description>
        <externalId>false</externalId>
        <label>Country Code</label>
        <length>25</length>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>IsActive__c</fullName>
        <defaultValue>true</defaultValue>
        <description>Indicates if the country region is available to the user for selection</description>
        <externalId>false</externalId>
        <label>Is Active</label>
        <trackTrending>false</trackTrending>
        <type>Checkbox</type>
    </fields>
    <fields>
        <fullName>Region_Code__c</fullName>
        <description>The ISO region code or abbreviation for the given country region value</description>
        <externalId>false</externalId>
        <label>Region Code</label>
        <length>25</length>
        <required>true</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Region_Name__c</fullName>
        <description>The name of the state, region, or province</description>
        <externalId>false</externalId>
        <label>Region Name</label>
        <length>255</length>
        <required>true</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <label>Country Region</label>
    <listViews>
        <fullName>All</fullName>
        <columns>NAME</columns>
        <columns>Country_Code__c</columns>
        <columns>IsActive__c</columns>
        <columns>Region_Code__c</columns>
        <columns>Region_Name__c</columns>
        <columns>CREATED_DATE</columns>
        <columns>UPDATEDBY_USER</columns>
        <columns>LAST_UPDATE</columns>
        <filterScope>Everything</filterScope>
        <label>All</label>
        <language>en_US</language>
    </listViews>
    <visibility>Protected</visibility>
</CustomObject>
