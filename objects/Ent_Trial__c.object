    <validationRules>
        <fullName>Omni_ConvertSales_Quote_Validation</fullName>
        <active>true</active>
        <description>Created as part of JIRA DPS-21221</description>
        <errorConditionFormula>AND(
ISCHANGED( Convert_to_Sale__c )
,
ISPICKVAL( Opportunity__r.Primary_Quote__r.Apttus_Proposal__Approval_Stage__c , &quot;Accepted&quot;)
)</errorConditionFormula>
        <errorMessage>Cannot change Convert to Sale once the Quote is Accepted</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Omni_ConvertSales_SSD_Validation</fullName>
        <active>true</active>
        <description>Created as part of DPS-21165.</description>
        <errorConditionFormula>OR(
AND( 
     Convert_to_Sale__c  = TRUE,
     ISPICKVAL( Trial_Status__c , &quot;Expired&quot;),
     Trial_End_Date__c - TODAY() &lt; 60,
     ISBLANK(Opportunity__r.Source_System_Detail__c )

),

AND( 
     Convert_to_Sale__c  = TRUE,
     ISPICKVAL( Trial_Status__c , &quot;Active&quot;),
     ISBLANK(Opportunity__r.Source_System_Detail__c )

  )
)</errorConditionFormula>
        <errorMessage>Please Create SSD on the Opportunity before converting the Trial to Sale.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Omni_ConvertSales_TImeLimit_Validation</fullName>
        <active>true</active>
        <description>Created as part of DPS-21164.</description>
        <errorConditionFormula>AND( 
     Convert_to_Sale__c  = TRUE,
     ISPICKVAL( Trial_Status__c , &quot;Expired&quot;),
     TODAY()- Trial_End_Date__c &gt;60
)</errorConditionFormula>
        <errorMessage>Exceeded Time Limit to Convert Trial to Sale.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Omni_Trial_ConvertSales_Validation</fullName>
        <active>true</active>
        <description>Created as part of DPS-21164.</description>
        <errorConditionFormula>AND( Convert_to_Sale__c  = TRUE,

    OR( 
        ISPICKVAL( Trial_Status__c , &quot;Draft&quot;),
        ISPICKVAL( Trial_Status__c , &quot;Rep Initiated&quot;),
        ISPICKVAL( Trial_Status__c , &quot;Invited&quot;),
        ISPICKVAL( Trial_Status__c , &quot;Bounceback&quot;),
        ISPICKVAL( Trial_Status__c , &quot;Entitlement Error&quot;),
        ISPICKVAL( Trial_Status__c , &quot;TimedOut&quot;),
        ISPICKVAL( Trial_Status__c , &quot;Accepted&quot;)
     )


)</errorConditionFormula>
        <errorMessage>Cannot Convert Trial to Sale. Trial is not Active.</errorMessage>
    </validationRules>
    <visibility>Public</visibility>
</CustomObject>
