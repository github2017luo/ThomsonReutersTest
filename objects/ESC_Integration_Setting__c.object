<?xml version="1.0" encoding="UTF-8"?>
<CustomObject xmlns="http://soap.sforce.com/2006/04/metadata">
    <customSettingsType>List</customSettingsType>
    <description>This provides a setting to maintain all standard integration settings (e.g. Endpoint, Timeout etc.)</description>
    <enableFeeds>false</enableFeeds>
    <fields>
        <fullName>ESC_Enable_Logging__c</fullName>
        <defaultValue>false</defaultValue>
        <description>Set to TRUE to enable logging of the request / response to the system debug log</description>
        <externalId>false</externalId>
        <label>Enable Logging</label>
        <trackTrending>false</trackTrending>
        <type>Checkbox</type>
    </fields>
    <fields>
        <fullName>ESC_Endpoint_Server__c</fullName>
        <description>This specifies the endpoint server for the integration - e.g. https://&lt;&lt;env&gt;&gt;.adobe-services.com/</description>
        <externalId>false</externalId>
        <label>Endpoint Server</label>
        <length>255</length>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>ESC_Endpoint_URI__c</fullName>
        <description>This species the URI for the integration service - e.g. territory_assignments/accounts/</description>
        <externalId>false</externalId>
        <label>Endpoint Create URI</label>
        <length>255</length>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>ESC_Endpoint_Update_URI__c</fullName>
        <externalId>false</externalId>
        <label>Endpoint Update URI</label>
        <length>255</length>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>ESC_HTTP_Headers__c</fullName>
        <description>Contains the map of the HTPP Headers (HeaderKey=&gt;HeaderValue) separated by semicolons following: 
HeaderKey=&gt;HeaderValue;HeaderKey=&gt;HeaderValue</description>
        <externalId>false</externalId>
        <inlineHelpText>Contains the map of the HTPP Headers (HeaderKey=&gt;HeaderValue) separated by semicolons following: 
HeaderKey=&gt;HeaderValue;HeaderKey=&gt;HeaderValue</inlineHelpText>
        <label>HTTP Headers</label>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>TextArea</type>
    </fields>
    <fields>
        <fullName>ESC_HTTP_Method__c</fullName>
        <description>This specifies the HTTP method - GET, POST etc.</description>
        <externalId>false</externalId>
        <label>HTTP Method</label>
        <length>255</length>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>ESC_Timeout__c</fullName>
        <externalId>false</externalId>
        <label>Timeout (Milliseconds)</label>
        <precision>18</precision>
        <required>false</required>
        <scale>0</scale>
        <trackTrending>false</trackTrending>
        <type>Number</type>
        <unique>false</unique>
    </fields>
    <label>ServiceNow Integration Setting</label>
    <visibility>Public</visibility>
</CustomObject>
