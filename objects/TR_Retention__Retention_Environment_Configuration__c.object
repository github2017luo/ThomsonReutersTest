<?xml version="1.0" encoding="UTF-8"?>
<CustomObject xmlns="http://soap.sforce.com/2006/04/metadata">
    <customSettingsType>List</customSettingsType>
    <description>This custom setting is used to hold the environment configuration for retention which will indicate whether instance supports both Apttus and Not-apttus users or just a single users.</description>
    <enableFeeds>false</enableFeeds>
    <fields>
        <fullName>TR_Retention__Apttus_Users__c</fullName>
        <defaultValue>false</defaultValue>
        <deprecated>false</deprecated>
        <externalId>false</externalId>
        <inlineHelpText>Check this field, if the instance has only Apttus users who are going to use Retention process. Keep it unchecked otherwise.</inlineHelpText>
        <label>Apttus Users</label>
        <trackTrending>false</trackTrending>
        <type>Checkbox</type>
    </fields>
    <fields>
        <fullName>TR_Retention__Has_Both_Users__c</fullName>
        <defaultValue>false</defaultValue>
        <deprecated>false</deprecated>
        <externalId>false</externalId>
        <inlineHelpText>Check this field, if the instance has both apttus and non-apttus users who are going to use Retention process.</inlineHelpText>
        <label>Has Both Users?</label>
        <trackTrending>false</trackTrending>
        <type>Checkbox</type>
    </fields>
    <label>Retention Environment Configuration</label>
    <visibility>Public</visibility>
</CustomObject>
