    <fields>
        <fullName>Contact_Language_Preference__c</fullName>
        <description>Shows Contact&apos;s Language Preference value, DPS-20244</description>
        <externalId>false</externalId>
        <formula>IF( 
	AND(ISPICKVAL(Contact_Name__r.ESC_tocLanguage__c, &quot;French-Canadian&quot;),  Contact_Name__r.LCRM_Country__c = &quot;CA&quot;),
	&apos;French (Canada)&apos;,
	IF(
		AND(ISPICKVAL(Contact_Name__r.ESC_tocLanguage__c, &quot;English&quot;),  Contact_Name__r.LCRM_Country__c = &quot;CA&quot;),
		&apos;English&apos;,
		IF(
			AND(ISPICKVAL(Contact_Name__r.ESC_tocLanguage__c, &quot;English&quot;),  Contact_Name__r.LCRM_Country__c = &quot;US&quot;),
			&apos;English (USA)&apos;,
			IF(
				AND(ISPICKVAL(Contact_Name__r.ESC_tocLanguage__c, &quot;English&quot;),  OR(Contact_Name__r.LCRM_Country__c = &quot;GB&quot;, Contact_Name__r.LCRM_Country__c = &quot;UK&quot;)),
				&apos;English (UK)&apos;,
				&apos;English (UK)&apos;
			)
		)
	)
)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Contact Language Preference</label>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
