    <webLinks>
        <fullName>Add_User</fullName>
        <availability>online</availability>
        <description>button to add OLC users on the trial page.</description>
        <displayType>button</displayType>
        <linkType>javascript</linkType>
        <masterLabel>Add User</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <url>var trialLead=&quot;{!Trial__c.Contact_LeadId__c}&quot; 
var isLeadConverted=&quot;{!Trial__c.Is_Lead_Converted__c}&quot; 
var email=&quot;{!Trial__c.ODT_Email__c}&quot; 
var trialStatus=&quot;{!Trial__c.Trial_Status__c}&quot; 
var trialSource=&quot;{!Trial__c.Trial_Source__c}&quot; 
var trialCategory=&quot;{!Trial__c.ODT_Trial_Product_Category__c}&quot; 
var trialId=&quot;{!Trial__c.Trial_Id__c}&quot; 
var GSI=&quot;{!Trial__c.GSI_Trial__c}&quot; 


if(&quot;{!$Profile.Name}&quot; == &quot;GLI Sales User&quot; || &quot;{!$Profile.Name}&quot; == &quot;GLI Sales Ops User&quot; || &quot;{!$Profile.Name}&quot; == &quot;System Administrator&quot; || &quot;{!$Profile.Name}&quot; == &quot;TR Sales&quot; || &quot;{!$Profile.Name}&quot; == &quot;TR Sales Ops&quot;) 

{ 
if(GSI==&apos;1&apos;) 
{ 
alert(&apos;{!$Label.GSI}&apos;);
}	
else if(trialStatus==&apos;&apos; || trialSource==&apos;&apos; || trialId==&apos;&apos;) 
{ 
alert(&apos;This Trial is not eligible to add users&apos;); 
} 
else 
if(trialLead!=&apos;&apos; &amp;&amp; isLeadConverted==&apos;0&apos; &amp;&amp; (&quot;{!$UserRole.Name}&quot;!=&quot;{!$Label.ODT_Trial_Manager_Role}&quot;)) 
{ 
alert(&apos;Please convert the lead associated to this trial before adding a user&apos;); 
} 
else 
{ 
window.open(&apos;/apex/ODT_TrialReInitiatePage?id={!Trial__c.Id}&amp;action=addUser&apos;,&apos;_self&apos;); 
} 
} 

else 
{ 
alert(&apos;You are not eligible to add users&apos;); 
}</url>
