    <validationRules>
        <fullName>Annual_Price_Non_Editable</fullName>
        <active>true</active>
        <description>Created as part of SFD-415</description>
        <errorConditionFormula>AND( $Setup.LTS_Disable_Application_Automation__c.LTS_Disable_Validation_Rules__c = false,
OR($Profile.Name = &quot;LatAm User&quot;,$Profile.Name = &quot;Partner Portal User&quot;), 
NOT(ISBLANK(Annual_Price__c )), 
OR( 
Opportunity.Renewal_Flag__c = true, 
AND( 
Opportunity.Short_Term_Sub__c &lt;&gt; true, 
NOT(OR( 
ISPICKVAL(Opportunity.Sales_Org__c,&quot;RV01&quot;), 
ISPICKVAL(Opportunity.Sales_Org__c,&quot;TSL&quot;), 
ISPICKVAL(Opportunity.Sales_Org__c,&quot;LPTC&quot;), 
ISPICKVAL(Opportunity.Sales_Org__c,&quot;LPCL&quot;),
ISPICKVAL(Opportunity.Sales_Org__c,&quot;DFCL&quot;)  
)) 
) 
) 
)</errorConditionFormula>
        <errorMessage>The Subscription is not marked as Non Annual. Please remove the value from the Annual Price field.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Annual_Price_Non_Editable_W_O_Short_Term</fullName>
        <active>true</active>
        <description>Created as part of SFD-415</description>
        <errorConditionFormula>AND( $Setup.LTS_Disable_Application_Automation__c.LTS_Disable_Validation_Rules__c = false,
OR($Profile.Name = &quot;LatAm User&quot;,$Profile.Name = &quot;Partner Portal User&quot;), 
NOT(ISBLANK(Annual_Price__c )), 
Opportunity.Short_Term_Sub__c &lt;&gt; true, 
OR( 
ISPICKVAL(Opportunity.Sales_Org__c,&quot;RV01&quot;), 
ISPICKVAL(Opportunity.Sales_Org__c,&quot;TSL&quot;), 
ISPICKVAL(Opportunity.Sales_Org__c,&quot;LPTC&quot;), 
ISPICKVAL(Opportunity.Sales_Org__c,&quot;LPCL&quot;),
ISPICKVAL(Opportunity.Sales_Org__c,&quot;DFCL&quot;) 
) 
)</errorConditionFormula>
        <errorMessage>The Annual Price field should not be populated for Full Term Subscriptions. If this is a Non Annual Subscription please check the Non Annual Sub field on the header before populating Annual Price.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Annual_Price_Req_for_Short_Term_Sub</fullName>
        <active>true</active>
        <description>Created as part of SFD-415</description>
        <errorConditionFormula>AND( $Setup.LTS_Disable_Application_Automation__c.LTS_Disable_Validation_Rules__c = false,
OR($Profile.Name = &quot;LatAm User&quot;,$Profile.Name = &quot;Partner Portal User&quot;), 
OR((ISPICKVAL( Opportunity.Sales_Org__c ,&quot;RV01&quot;)), 
(ISPICKVAL( Opportunity.Sales_Org__c ,&quot;TSL&quot;)), 
(ISPICKVAL( Opportunity.Sales_Org__c ,&quot;LPTC&quot;)), 
(ISPICKVAL( Opportunity.Sales_Org__c ,&quot;LPCL&quot;)),
(ISPICKVAL( Opportunity.Sales_Org__c ,&quot;DFCL&quot;))), 
ISBLANK( Annual_Price__c ), Opportunity.Short_Term_Sub__c = true ,Opportunity.Renewal_Flag__c &lt;&gt; true 
)</errorConditionFormula>
        <errorMessage>The Annual Price field is required for Non Annual Subscriptions.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>LATAM_OLI_Lock_to_Sales_atOpty_0</fullName>
        <active>true</active>
        <description>Lock the opportunity line Item when the opportunity sales stage is 0%</description>
        <errorConditionFormula>AND($Setup.LTS_Disable_Application_Automation__c.LTS_Disable_Validation_Rules__c = false,
        $User.Username &lt;&gt; $Label.InformaticaUserName,
	NOT(ISPICKVAL( Opportunity.Sales_Org__c ,&quot;SFWS&quot;)), 
	NOT(ISPICKVAL( Opportunity.Sales_Org__c ,&quot;SCSI&quot;)), 
	NOT(ISPICKVAL( Opportunity.Sales_Org__c ,&quot;TSCE&quot;)), 
	NOT(ISPICKVAL( Opportunity.Sales_Org__c ,&quot;EDIC&quot;)), 
	ISPICKVAL(Opportunity.StageName,&quot;0%&quot;),  
	OR( $Profile.Name = &quot;LatAm User&quot;, $Profile.Name = &quot;Partner Portal User&quot;), 
	$UserRole.Name &lt;&gt; &quot;LatAm Order Fulfillment&quot;, 
	Opportunity.RecordType.Name = &apos;LATAM Renewal Opportunity&apos;,
	Opportunity.Renewal_Flag__c = true
)</errorConditionFormula>
        <errorMessage>Opportunity Line Item should be fully locked if the Opportunity is 0%</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>LATAM_OLI_Lock_to_Sales_at_Opty_95_100</fullName>
        <active>true</active>
        <description>Opportunity Line Item should be fully locked if the Opportunity is 95% and 100%.</description>
        <errorConditionFormula>AND($Setup.LTS_Disable_Application_Automation__c.LTS_Disable_Validation_Rules__c = false,
        $User.Username &lt;&gt; $Label.InformaticaUserName ,
	OR(ISPICKVAL(Opportunity.StageName,&quot;100%&quot;),ISPICKVAL(Opportunity.StageName,&quot;95%&quot;)) ,
	OR($Profile.Name = &quot;LatAm User&quot;,$Profile.Name = &quot;Partner Portal User&quot;),
	$UserRole.DeveloperName &lt;&gt; &quot;LatAm_Order_Fulfillment&quot;, 
	NOT(ISPICKVAL(Opportunity.Oppty_Mgmt_Type__c, &quot;Transaction Summary&quot;)) 
)</errorConditionFormula>
        <errorMessage>If the opportunity is at a stage of 95% or 100%, Line Item cannot be edited by sales; only by the Order processing team.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>LATAM_Renewals_OLI_Lock_at_Quote_Remove</fullName>
        <active>true</active>
        <description>[FRS 3.4.7]Opportunity Line Item should be fully locked if the Quote Status is Remove or Cancel</description>
        <errorConditionFormula>AND( $Setup.LTS_Disable_Application_Automation__c.LTS_Disable_Validation_Rules__c = false,
	$User.Username &lt;&gt; $Label.InformaticaUserName , 
	OR( $Profile.Name = &quot;LatAm User&quot;, $Profile.Name = &quot;Partner Portal User&quot;),	
	Opportunity.RecordType.DeveloperName = &apos;LATAM_Renewal_Opportunity&apos;, 
	Opportunity.Renewal_Flag__c = true,
	OR(	
		AND( 
			ISPICKVAL(Opportunity.Approval_Status__c, &quot;Pending Cancellation Approval&quot;),
			Opportunity.IsSkipOppLockingValidation__c = false	
		), 
		AND( 
			ISPICKVAL(Opportunity.Approval_Status__c, &quot;Cancellation Approved&quot;),			
			Opportunity.IsSkipOppLockingValidation__c = false, 
			$UserRole.DeveloperName &lt;&gt; &quot;LatAm_Order_Fulfillment&quot; 
		),
		AND( 
			ISPICKVAL(Opportunity.StageName, &quot;0%&quot;),
			ISPICKVAL(Opportunity.Status__c, &quot;Discarded&quot;)
		),
		AND( 
			ISPICKVAL(Opportunity.Approval_Status__c, &quot;Pending Removal&quot;), 						
			Opportunity.IsSkipOppLockingValidation__c = false	
		), 
		AND( 
			ISPICKVAL(Opportunity.Approval_Status__c, &quot;Removed&quot;),						
			Opportunity.IsSkipOppLockingValidation__c = false	
		)
	)	
)</errorConditionFormula>
        <errorMessage>If the Quote is submitted for Cancel or Remove, Line Item cannot be edited.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>LCRM_Cancel_Reason_Mandatory_Renewals</fullName>
