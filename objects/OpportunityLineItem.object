    </validationRules>
    <validationRules>
        <fullName>LCRM_Renewals_Pro_data_validation_rule</fullName>
        <active>true</active>
        <description>CRM-2113</description>
        <errorConditionFormula>AND($Setup.LTS_Disable_Application_Automation__c.LTS_Disable_Validation_Rules__c = false,
 ISCHANGED( LCRM_Action__c ) ,
Opportunity.LCRM_Sub_BU__c = &quot;UKI&quot;,
        Opportunity.Is_GLI__c ,
     OR(
           AND(
		        ISPICKVAL( PRIORVALUE( LCRM_Action__c ),&quot;Add&quot;),
                NOT(ISPICKVAL(LCRM_Action__c,&quot;Remove&quot;))
				),
				
			AND(
			ISPICKVAL( PRIORVALUE( LCRM_Action__c ),&quot;Remove&quot;),
			NOT(ISPICKVAL(LCRM_Action__c,&quot;Add&quot;))
			),
           AND(
            (ISPICKVAL( PRIORVALUE( LCRM_Action__c ),&quot;Renew&quot;)),
                NOT(ISPICKVAL(LCRM_Action__c,&quot;Cancel&quot;))
              ),
		   AND(
            (ISPICKVAL( PRIORVALUE( LCRM_Action__c ),&quot;Cancel&quot;)),
                NOT(ISPICKVAL(LCRM_Action__c,&quot;Renew&quot;))
              )
       ))</errorConditionFormula>
