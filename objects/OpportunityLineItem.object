        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/26.0/connection.js&quot;)} 
{!REQUIRESCRIPT(&quot;/soap/ajax/26.0/apex.js&quot;)}
{!REQUIRESCRIPT(&quot;/support/console/26.0/integration.js&quot;)}
var OppId= &apos;{!Opportunity.Id}&apos;; 
var URL = &apos;{!$Site.Prefix}&apos;;
var PriceApproved=&quot;Pricing Approved&quot;;
var Pendingmanager=&quot;Pending Manager Approval&quot;;
