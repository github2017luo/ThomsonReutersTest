    <validationRules>
        <fullName>LatAm_OptyLI_Subscription_Dates_Req</fullName>
        <active>true</active>
        <description>For only LatAm Profiles  (and not in-flight) this requires that the Expected Revenue Date and Expiration Date be populated if:  Product has Subscribable Indicator value of Subscribable
		Option 2:  The Opportunity Category is SUSCRIPCION</description>
        <errorConditionFormula>AND($Setup.LTS_Disable_Application_Automation__c.LTS_Disable_Validation_Rules__c = false,

		 OR (
		 ISPICKVAL( Opportunity.Sales_Org__c  ,&quot;LLEY&quot;), 
		 ISPICKVAL( Opportunity.Sales_Org__c ,&quot;LLPA&quot;),
		 ISPICKVAL( Opportunity.Sales_Org__c ,&quot;LLUR&quot;),
		 ISPICKVAL( Opportunity.Sales_Org__c ,&quot;BEJE&quot;),
                 ISPICKVAL( Opportunity.Sales_Org__c,&quot;DFCL&quot;)
 ),
		OR(
		ISNULL(Opportunity.Expected_Revenue_Date__c),
		ISNULL(Opportunity.Expiration_Date__c)),
		OR(
		$Profile.Name = &quot;LatAm User&quot;,
		$Profile.Name = &quot;Partner Portal User&quot;),
		OR(
		Opportunity.Has_LatAm_Stabilization_Primary__c &gt; 0,
		Opportunity.All_Quote_Count__c = 0,Opportunity.RecordType.Name = &apos;LATAM Renewal Opportunity&apos;),
		ISPICKVAL(PricebookEntry.Product2.Subscribable_Indicator__c,&quot;Subscribable&quot;),Opportunity.Is_Child_Opportunity__c &lt;&gt; true 
		)</errorConditionFormula>
        <errorMessage>Expected Revenue Date and Expiration Date cannot be blank. Please, review and include a value for both fields.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>LatAm_Opty_LI_Subscription_Date_Sequence</fullName>
        <active>true</active>
        <description>For application LatAm Profiles (and not in-flight) only this requires that the Expected Revenue Date be Prior to the Expiration Date if  Product has Subscribable Indicator value of Subscribable .</description>
        <errorConditionFormula>AND($Setup.LTS_Disable_Application_Automation__c.LTS_Disable_Validation_Rules__c = false,
Opportunity.Expected_Revenue_Date__c &gt;= Opportunity.Expiration_Date__c,
OR(
$Profile.Name = &quot;LatAm User&quot;,
$Profile.Name = &quot;Partner Portal User&quot;),
OR(
Opportunity.Has_LatAm_Stabilization_Primary__c &gt; 0,
Opportunity.All_Quote_Count__c = 0,Opportunity.RecordType.Name = &apos;LATAM Renewal Opportunity&apos;),
ISPICKVAL(PricebookEntry.Product2.Subscribable_Indicator__c,&quot;Subscribable&quot;) 
)</errorConditionFormula>
        <errorMessage>Expected Revenue Date cannot be more than or equal to the Expiration Date. Please, review and correct the dates in those fields.</errorMessage>
    </validationRules>
