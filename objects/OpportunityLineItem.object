var result = sforce.connection.query(&quot;Select Id, Approval_Status__c from Quote where Primary_Quote__c =True and (Approval_Status__c=&apos;&quot;+PriceApproved+&quot;&apos; or Approval_Status__c=&apos;&quot;+Pendingmanager+&quot;&apos; or Approval_Status__c=&apos;&quot;+PendingPriceApproval+&quot;&apos;) and OpportunityId = &apos;&quot;+OppId+&quot;&apos;&quot;);
var records =result.getArray(&quot;records&quot;);

if(records.length &gt;0 &amp;&amp; records[0].Id != null ){
	
  var approvalStatus= records[0].Approval_Status__c;
	
    if(approvalStatus == &apos;Pricing Approved&apos;){             
      window.alert(&apos;{!$Label.Add_Edit_Delete_Line_Item}&apos;); 
     } 
    else if(approvalStatus == &apos;Pending Manager Approval&apos;){             
      window.alert(&apos;{!$Label.Add_Edit_Quote_line_Item}&apos;); 
     }
    else if(approvalStatus == &apos;Pending Pricing Approval&apos;){             
      window.alert(&apos;{!$Label.Add_Quote_line_Item_While_Pending_Approval}&apos;); 
     }
}
else{ 
	if (sforce.console.isInConsole()) { 
    URL += URL+&apos;/apex/ProductSelection?id={!Opportunity.Id}&amp;isdtp=vw&apos;; 
    srcSelf(URL);
	}
	else { 
		window.parent.location.href = (URL+&apos;/apex/ProductSelection?id={!Opportunity.Id}&apos;);
	}
	
    
}</url>
