    <validationRules>
        <fullName>pse__Invoiced</fullName>
        <active>true</active>
        <description>Invoice Number or Date is not allowed on record unless it is invoiced</description>
        <errorConditionFormula>(pse__Invoice_Number__c != null ||
pse__Invoice_Date__c != null) &amp;&amp; pse__Invoiced__c = false</errorConditionFormula>
        <errorMessage>Invoice Number or Date is not allowed on record unless it is invoiced</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>pse__Milestone_Billing_Requires_Inclusion</fullName>
        <active>true</active>
        <description>For a Milestone to be marked as Billed, it must also be marked as Included In Financials and Approved.</description>
        <errorConditionFormula>AND(pse__Billed__c,OR(NOT(pse__Include_In_Financials__c),NOT(pse__Approved__c)))</errorConditionFormula>
        <errorDisplayField>pse__Billed__c</errorDisplayField>
        <errorMessage>For a Milestone to be marked as Billed, it must also be marked as Included In Financials and Approved.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>pse__Milestone_Invoicing_Requires_Billing</fullName>
        <active>true</active>
        <description>For a Milestone to be marked as Invoiced, it must also be marked as Billed.</description>
        <errorConditionFormula>AND(pse__Invoiced__c,NOT(pse__Billed__c))</errorConditionFormula>
        <errorDisplayField>pse__Invoiced__c</errorDisplayField>
        <errorMessage>For a Milestone to be marked as Invoiced, it must also be marked as Billed.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>pse__Milestone_Project_is_Constant</fullName>
        <active>true</active>
        <description>A Milestone&apos;s Project field value may not be blank and may not be updated once set (unless Admin Global Edit is checked, Audit Notes are provided, and Actuals Calculation Mode is Scheduled).</description>
        <errorConditionFormula>OR(ISNULL(pse__Project__c),(pse__Project__c=&apos;&apos;),AND(ISCHANGED(pse__Project__c),OR(NOT(pse__Admin_Global_Edit__c),ISNULL(pse__Audit_Notes__c),ISBLANK(TRIM(pse__Audit_Notes__c)))))</errorConditionFormula>
        <errorDisplayField>pse__Project__c</errorDisplayField>
        <errorMessage>A Milestone&apos;s Project field value may not be blank and may not be updated once set (unless Admin Global Edit is checked, Audit Notes are provided, and Actuals Calculation Mode is Scheduled).</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>pse__Vendor_Invoice_Constant</fullName>
        <active>true</active>
        <errorConditionFormula>AND(ISCHANGED(pse__Vendor_Invoice_Item__c),LEN(PRIORVALUE(pse__Vendor_Invoice_Item__c))&gt;0)</errorConditionFormula>
        <errorDisplayField>pse__Vendor_Invoice_Item__c</errorDisplayField>
        <errorMessage>Vendor Invoice Item cannot be changed</errorMessage>
    </validationRules>
