        <availability>online</availability>
        <displayType>massActionButton</displayType>
        <linkType>javascript</linkType>
        <masterLabel>New Strategy</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <requireRowSelection>false</requireRowSelection>
        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/50.0/connection.js&quot;)} 
{!REQUIRESCRIPT(&quot;/soap/ajax/50.0/apex.js&quot;)} 
sforce.connection.sessionId = &apos;{!$Api.Session_ID}&apos;;

var qr = sforce.connection.query(&quot;select id,Name from SFDC_Acct_Plan__c where Account__c =&apos;{!Opportunity.AccountId}&apos; &quot;) ;
var records = qr.getArray(&quot;records&quot;);

if(qr.size&gt;0){
window.top.location = &quot;/a09/e?retURL=%2F{!Opportunity.Id}&amp;{!$Setup.FieldId__c.AccountNameOnStrategy__c}={!Opportunity.Account}&amp;{!$Setup.FieldId__c.AccountOnStrategy__c}={!Opportunity.AccountId}&amp;{!$Setup.FieldId__c.OppNameOnStrategy__c}={!Opportunity.Name}&amp;{!$Setup.FieldId__c.OppOnStrategy__c}={!Opportunity.Id}&amp;{!$Setup.FieldId__c.AccPlanOnStrategy__c}=&quot;+records[0].Id+&quot;&amp;{!$Setup.FieldId__c.AccPlanNameOnStrategy__c}=&quot;+records[0].Name
}else{
window.top.location = &quot;/a09/e?retURL=%2F{!Opportunity.Id}&amp;{!$Setup.FieldId__c.AccountNameOnStrategy__c}={!Opportunity.Account}&amp;{!$Setup.FieldId__c.AccountOnStrategy__c}={!Opportunity.AccountId}&amp;{!$Setup.FieldId__c.OppNameOnStrategy__c}={!Opportunity.Name}&amp;{!$Setup.FieldId__c.OppOnStrategy__c}={!Opportunity.Id}&quot;
}</url>
    </webLinks>
