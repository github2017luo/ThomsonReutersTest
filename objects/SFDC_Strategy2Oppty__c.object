        <url>/006/e?retURL=%2F{!SFDC_Strategy__c.Id}&amp;RecordType={!$Setup.FieldId__c.Pipeline_Record_Type__c}&amp;{!$Setup.FieldId__c.OppStrategy__c}={!SFDC_Strategy__c.Id}&amp;{!$Setup.FieldId__c.OppStrategyName__c}={!SFDC_Strategy__c.Name}&amp;opp4_lkid={!SFDC_Strategy__c.AccountId__c}&amp;opp4={!SFDC_Strategy__c.Account__c}&amp;ent=Opportunity</url>
