    <description>Used to update Case feeds related to feed Items</description>
    <formulas>
        <processMetadataValues>
            <name>originalFormula</name>
            <value>
                <stringValue>AND(
$Setup.LTS_Disable_Application_Automation__c.LTS_Disable_Process_Builders__c = false,
	AND(
	[FeedItem].Parent:Case.Is_ESC_Case__c,
	BEGINS([FeedItem].ParentId, &apos;500&apos;)
	),
	OR(
	(ISPICKVAL([FeedItem].Type,&apos;Text Post&apos;)), 
	(ISPICKVAL([FeedItem].Type,&apos;Content Post&apos;)), 
	(ISPICKVAL([FeedItem].Type,&apos;Link Post&apos;))
	),
	NOT(ISNULL([FeedItem].CreatedBy:User.ContactId))
)</stringValue>
            </value>
        </processMetadataValues>
        <name>formula_myRule_1</name>
        <dataType>Boolean</dataType>
        <expression>AND(
{!$Setup.LTS_Disable_Application_Automation__c.LTS_Disable_Process_Builders__c} = false,
	AND(
	{!myVariable_current.Parent:Case.Is_ESC_Case__c},
	BEGINS({!myVariable_current.ParentId}, &apos;500&apos;)
	),
	OR(
	(ISPICKVAL({!myVariable_current.Type},&apos;Text Post&apos;)), 
	(ISPICKVAL({!myVariable_current.Type},&apos;Content Post&apos;)), 
	(ISPICKVAL({!myVariable_current.Type},&apos;Link Post&apos;))
	),
	NOT(ISNULL({!myVariable_current.CreatedBy:User.ContactId}))
)</expression>
    </formulas>
    <formulas>
        <processMetadataValues>
            <name>originalFormula</name>
            <value>
                <stringValue>AND(
$Setup.LTS_Disable_Application_Automation__c.LTS_Disable_Process_Builders__c = false,
    [FeedItem].Parent:Case.OwnerId == $User.USERID18__c ,
	[FeedItem].Parent:Case.Is_ESC_Case__c,
	BEGINS([FeedItem].ParentId, &apos;500&apos;)
)</stringValue>
            </value>
        </processMetadataValues>
        <name>formula_myRule_3</name>
        <dataType>Boolean</dataType>
        <expression>AND(
$Setup.LTS_Disable_Application_Automation__c.LTS_Disable_Process_Builders__c = false,
    {!myVariable_current.Parent:Case.OwnerId} == $User.USERID18__c ,
	{!myVariable_current.Parent:Case.Is_ESC_Case__c},
	BEGINS({!myVariable_current.ParentId}, &apos;500&apos;)
)</expression>
    </formulas>
    <formulas>
        <processMetadataValues>
            <name>originalFormula</name>
            <value>
                <stringValue>AND(
$Setup.LTS_Disable_Application_Automation__c.LTS_Disable_Process_Builders__c = false,
    [FeedItem].Parent:Case.Is_ESC_Case__c == True ,
    [FeedItem].Parent:Case.IsClosed == True ,
	[FeedItem].Parent:Case.Is_ESC_Case__c,
	([FeedItem].Parent:Case.OwnerId &lt;&gt; $User.USERID18__c)
)</stringValue>
            </value>
        </processMetadataValues>
        <name>formula_myRule_5</name>
        <dataType>Boolean</dataType>
        <expression>AND(
$Setup.LTS_Disable_Application_Automation__c.LTS_Disable_Process_Builders__c = false,
    {!myVariable_current.Parent:Case.Is_ESC_Case__c} == True ,
    {!myVariable_current.Parent:Case.IsClosed} == True ,
	{!myVariable_current.Parent:Case.Is_ESC_Case__c},
	({!myVariable_current.Parent:Case.OwnerId} &lt;&gt; $User.USERID18__c)
)</expression>
    </formulas>
