    <formulas>
        <processMetadataValues>
            <name>originalFormula</name>
            <value>
                <stringValue>$Label.ANZ_Business_Hour_ID </stringValue>
            </value>
        </processMetadataValues>
        <name>formula_2_myRule_1_A1_5879815418</name>
        <dataType>String</dataType>
        <expression>{!$Label.ANZ_Business_Hour_ID}</expression>
    </formulas>
    <formulas>
        <processMetadataValues>
            <name>originalFormula</name>
            <value>
                <stringValue>AND ( $Setup.LTS_Disable_Application_Automation__c.LTS_Disable_Process_Builders__c = false,
OR( ISNEW(), 
	AND(ISCHANGED([Contract].CustomerSignedDate) ,
		ISCHANGED([Contract].CustomerSignedId)) ),
NOT(ISNULL([Contract].CustomerSignedDate)),
NOT(ISNULL([Contract].CustomerSignedId)),
OR(ISPICKVAL([Contract].Account.Sales_Org__c,&quot;BKRS&quot;), 
ISPICKVAL([Contract].Account.Sales_Org__c,&quot;LRA&quot;))
)</stringValue>
            </value>
        </processMetadataValues>
        <name>formula_myRule_1</name>
        <dataType>Boolean</dataType>
        <expression>AND ( {!$Setup.LTS_Disable_Application_Automation__c.LTS_Disable_Process_Builders__c} = false,
OR( ISNEW(), 
	AND(ISCHANGED({!myVariable_current.CustomerSignedDate}) ,
		ISCHANGED({!myVariable_current.CustomerSignedId})) ),
NOT(ISNULL({!myVariable_current.CustomerSignedDate})),
NOT(ISNULL({!myVariable_current.CustomerSignedId})),
OR(ISPICKVAL({!myVariable_current.Account.Sales_Org__c},&quot;BKRS&quot;), 
ISPICKVAL({!myVariable_current.Account.Sales_Org__c},&quot;LRA&quot;))
)</expression>
    </formulas>
    <formulas>
        <processMetadataValues>
            <name>originalFormula</name>
            <value>
                <stringValue>AND ( $Setup.LTS_Disable_Application_Automation__c.LTS_Disable_Process_Builders__c = false,
            ISCHANGED([Contract].CustomerSignedDate),
            NOT(ISNULL([Contract].CustomerSignedDate)),
OR(ISPICKVAL([Contract].Account.Sales_Org__c,&quot;BKRS&quot;), 
ISPICKVAL([Contract].Account.Sales_Org__c,&quot;LRA&quot;)),
ISPICKVAL(PRIORVALUE([Contract].Status),&quot;Approved&quot;)
)</stringValue>
            </value>
        </processMetadataValues>
        <name>formula_myRule_4</name>
        <dataType>Boolean</dataType>
        <expression>AND ( {!$Setup.LTS_Disable_Application_Automation__c.LTS_Disable_Process_Builders__c} = false,
            ISCHANGED({!myVariable_current.CustomerSignedDate}),
            NOT(ISNULL({!myVariable_current.CustomerSignedDate})),
OR(ISPICKVAL({!myVariable_current.Account.Sales_Org__c},&quot;BKRS&quot;), 
ISPICKVAL({!myVariable_current.Account.Sales_Org__c},&quot;LRA&quot;)),
ISPICKVAL(PRIORVALUE({!myVariable_current.Status}),&quot;Approved&quot;)
)</expression>
    </formulas>
