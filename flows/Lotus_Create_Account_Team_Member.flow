    </formulas>
    <formulas>
        <processMetadataValues>
            <name>originalFormula</name>
            <value>
                <stringValue>AND(
$Setup.LOTUS_Lightning_Profiles__c.End_State_Lightning__c,
[Lead].IsConverted = true,
[Lead].Is_GLI__c  = true,
ISPICKVAL([Lead].LeadSource , &quot;Partner&quot;) ,
NOT(ISNULL([Lead].ConvertedAccountId)),
NOT(ISNULL([Lead].CreatedById )) 
)</stringValue>
            </value>
        </processMetadataValues>
        <name>formula_myRule_3</name>
        <dataType>Boolean</dataType>
        <expression>AND(
{!$Setup.LOTUS_Lightning_Profiles__c.End_State_Lightning__c},
{!myVariable_current.IsConverted} = true,
{!myVariable_current.Is_GLI__c}  = true,
ISPICKVAL({!myVariable_current.LeadSource} , &quot;Partner&quot;) ,
NOT(ISNULL({!myVariable_current.ConvertedAccountId})),
NOT(ISNULL({!myVariable_current.CreatedById} )) 
)</expression>
    </formulas>
