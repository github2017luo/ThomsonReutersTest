    <description>For UKI, When Pro-rata transaction Oppty, update AccountId, Key Contact and Expected End Date from Target Oppty</description>
    <formulas>
        <processMetadataValues>
            <name>originalFormula</name>
            <value>
                <stringValue>$Setup.LTS_Disable_Application_Automation__c.LTS_Disable_Process_Builders__c</stringValue>
            </value>
        </processMetadataValues>
        <name>formula_myRule_1</name>
        <dataType>Boolean</dataType>
        <expression>{!$Setup.LTS_Disable_Application_Automation__c.LTS_Disable_Process_Builders__c}</expression>
    </formulas>
    <formulas>
        <processMetadataValues>
            <name>originalFormula</name>
            <value>
                <stringValue>AND(NOT($Setup.LTS_Disable_Application_Automation__c.LTS_Disable_Process_Builders__c),
      [Opportunity].Is_GLI__c,
      [Opportunity].LCRM_Sub_BU__c = &apos;UKI&apos;,
      NOT(ISBLANK([Opportunity].LCRM_Target_Opportunity__c)),
      OR(
              ISNEW(),
	      ISCHANGED([Opportunity].LCRM_Target_Opportunity__c )
         )
    )</stringValue>
            </value>
        </processMetadataValues>
        <name>formula_myRule_3</name>
        <dataType>Boolean</dataType>
        <expression>AND(NOT({!$Setup.LTS_Disable_Application_Automation__c.LTS_Disable_Process_Builders__c}),
      {!myVariable_current.Is_GLI__c},
      {!myVariable_current.LCRM_Sub_BU__c} = &apos;UKI&apos;,
      NOT(ISBLANK({!myVariable_current.LCRM_Target_Opportunity__c})),
      OR(
              ISNEW(),
	      ISCHANGED({!myVariable_current.LCRM_Target_Opportunity__c} )
         )
    )</expression>
    </formulas>
    <formulas>
        <processMetadataValues>
